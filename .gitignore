int dobra (int val) {
    cout &lt;&lt; ": ";
    return sqrt(pow(val,2));
}
 
void detona (int vet[], int soma) {
    for (int i = 1; i &lt; 4; i++) {
        vet[i] = 0;
        soma = soma + i;
    }
}
 
int main()
{
    int vetor[] = {3,1,5,8,1,8}, i, soma;
    soma = vetor[0];
 
    for (i = 1; i &lt; 4; i++) {
        soma = (soma + vetor[i]) * -1;
    }
    cout &lt;&lt; soma;
    cout &lt;&lt; dobra(soma);
    detona(vetor, soma);
    cout &lt;&lt; endl;
    cout &lt;&lt; vetor[0] &lt;&lt; ": " &lt;&lt; soma;
}
